{
	"$schema": "https://json.schemastore.org/package",
	"name": "repetier-server-client",
	"version": "0.0.1",
	"description": "A Typescript client for Repetier Server",
	"engines": {
		"node": ">=19.0.0"
	},
	"engineStrict": true,
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"keywords": [
		"repetier",
		"3d printing",
		"additive manufacturing"
	],
	"author": "James Gopsill",
	"license": "MIT",
	"scripts": {
		"test": "jest",
		"format": "prettier --write .",
		"example": "ts-node --esm example/example.ts",
		"build:dist": "rm -rf dist && tsc --project tsconfig.json && tsc --project tsconfig.cjs.json && node create-package-jsons.mjs",
		"build:docs": "typedoc"
	},
	"devDependencies": {
		"@tsconfig/node18-strictest": "^1.0.0",
		"@tsconfig/node18-strictest-esm": "^1.0.1",
		"@types/jest": "^29.4.0",
		"@types/node": "^18.13.0",
		"@types/uuid": "^9.0.0",
		"jest": "^29.4.1",
		"prettier": "^2.8.3",
		"prettier-plugin-organize-imports": "^3.2.2",
		"ts-jest": "^29.0.5",
		"typedoc": "^0.23.24",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"isomorphic-ws": "^5.0.0",
		"uuid": "^9.0.0",
		"ws": "^8.12.0"
	}
}
